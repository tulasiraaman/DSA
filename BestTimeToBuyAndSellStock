class Solution {
    public int maxProfit(int[] prices) {
        int n = prices.length;
        if(n == 0) return -1;

        int min_price = prices[0];
        int profit = 0;

        for(int i = 1;i < n;i++){
            min_price = Math.min(min_price, prices[i]);
            profit = Math.max(profit, prices[i] - min_price);
            System.out.println(profit);
        }
        return profit;
    }
}
