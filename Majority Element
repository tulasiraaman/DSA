class Solution {
    public int majorityElement(int[] nums) {

        int count = 0, majority = 0, n = nums.length;
        if(n == 0) return majority;
        for(int i = 0;i < n;i++){
            if(count == 0){
                majority = nums[i];
            }
            if(majority == nums[i]){
                count++;
            }else{
                count--;
            }
        }

        return majority;
    }
}
